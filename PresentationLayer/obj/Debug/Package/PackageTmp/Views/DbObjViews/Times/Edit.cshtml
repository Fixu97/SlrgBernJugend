@model Shared.Models.db.TimeDTO
<tr>
    <th>
        Person
    </th>
    <td id ="td-FK_P">
    </td>
</tr>
<tr>
    <th>
        Discipline
    </th>
    <td id="td-FK_D">
    </td>
</tr>
<tr>
    <th>
        Date
    </th>
    <td>
        @Html.TextBoxFor(model => Model.Date, new { Value = Model.Date.ToString("dd.MM.yyyy") })
    </td>
</tr>
<tr>
    <th>
        Seconds
    </th>
    <td>
        @Html.TextBoxFor(model => Model.Seconds)
    </td>
</tr>
<script>
    $(document).ready(function() {
        CreatePeopleDropdown(@Model.FK_P, $("#td-FK_P"));
        CreateDisciplinesDropdown(@Model.FK_D, $("#td-FK_D"));
    });

</script>