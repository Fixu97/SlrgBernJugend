@model Shared.Models.db.DisciplineDTO

<tr>
    <th>
        Discipline
    </th>
    <td>
        @Html.Raw(Model.DiscName)
    </td>
</tr>
<tr>
    <th>
        Meters
    </th>
    <td>
        @Html.Raw(Model.Meters)
    </td>
</tr>
<script>
    $(document).ready(function () {
        var btnEdit = $($("#edit")[0]);
        var btnCreateByDisc = $("<button class='btn btn-primary' style='float:right;'onclick='document.location=\"/Times/Wizard/@Model.Pk\"' title='Insert times for this discipline'>Insert times...</button>");

        btnEdit.after(btnCreateByDisc);
    });
</script>