@model Shared.Models.db.RelayDTO

<tr>
    <th>
        Relay Name
    </th>
    <td>
        @Html.Raw(Model.Relay)
    </td>
</tr>
<tr>
    <th>Disciplines</th>
    <td>
        <ol>
        @foreach (var disc in Model.SortedDisciplines)
        {
            <li>@disc.DisplayName</li>
        }
    </ol>
    </td>
</tr>